import java.util.Scanner;

public class ColorSwap {
   
        public static void main(String[] args) 
        {
        Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
        int a=0;
        int b=1;
        fibonacci(n,a,b);
        }
        public static void fibonacci(int n,int a,int b){
               if(n==0)
               {
                  return;
               }
                System.out.println(a);
                n=n-1;
                fibonacci(n,b,a+b);

        }
    }
